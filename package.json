{
  "name": "valp-syntax",
  "displayName": "valp-syntax",
  "description": "Syntax highligh for Valp language",
  "icon": "images/valp-logo.png",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
		"type": "git",
		"url": "https://github.com/valp-lang/valp-vscode"
	},
	"bugs": "https://github.com/valp-lang/valp-vscode/issues",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "valp",
      "aliases": ["Valp", "valp"],
      "extensions": [".vp"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "valp",
      "scopeName": "source.vp",
      "path": "./syntaxes/valp.tmLanguage.json"
    }]
  }
}
